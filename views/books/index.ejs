<!-- this is where /books will go -->
<%- include('../partials/header') %>
    <p><a href="/">Go Home</a></p>
    <h1>Bookshelf</h1>
    <p>You have a total <%= books.length %> books on your shelf.</p>
    <ul id="book-list">
        <% books.forEach(book => { %>
            <li><a href="/books/<%= book._id %>"><%= book.title %> by <%= book.author %></a></li>
        <% }); %>
    </ul>

</body>
</html>