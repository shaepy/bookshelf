<%- include('../../partials/header') %>
    <p><a href="/" class="nav-link">← Go Home</a></p>
    <h1 class="dm-serif-display">Books Owned</h1>
    <%- include('../../partials/nav') %>
    <h2 class="dm-serif-display">You own <%= books.length %> books 📚</h2>
    <ul class="book-list">
        <% books.forEach(book => { %>
            <li><a href="/books/<%= book._id %>"><%= book.title %> by <%= book.author %></a></li>
        <% }); %>
    </ul>
</body>
